{
	"$schema": "vscode://schemas/color-theme",
	"name": "Light+ with Autolisp'",
	"include": "./light_plus.json",
	"tokenColors": [
		//autolisp-prime
		{
			"name": "autolisp-prime",
			"scope": [
				"keyword.atom.autolisp-prime",
				"keyword.const.autolisp-prime",
				"keyword.enum.ac.autolisp-prime",
				"keyword.enum.vlax.vb.autolisp-prime",
				"keyword.func.acet.autolisp-prime",

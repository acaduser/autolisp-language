			"settings": {
				"foreground": "#0000ff"
			}
		},
		{
			"name": "autolispdcl-prime",
			"scope": [
				"tile.autolispdcl-prime",
			],
			"settings": {
				"foreground": "#ae00ff"
			}
		},
		{
			"name": "autolispdcl-prime",
			"scope": [
				"constant.numeric.autolispdcl-prime",
			],
			"settings": {
				"foreground": "#b87905"
			}
		},
		{
			"name": "autolispdcl-prime",
			"scope": [
				"colon.autolispdcl-prime"
			],
			"settings":{
				"foreground": "#ff0000"
			}
		},
		{
			"name": "autolispdcl-prime",
			"scope": [
				"semicolon.autolispdcl-prime"
			],
			"settings":{
				"foreground": "#ff0000"
			}

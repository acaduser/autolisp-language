{
	"name": "autolisp-language",
	"displayName": "autolisp language support",
	"description": "autolisp language syntax highlight and function input support",
	"version": "1.1.12",
	"publisher": "jpnacaduser",
	"engines": {
		"vscode": "^1.25.0"
	},
	"categories": [
		"Programming Languages",
		"Themes",
		"Snippets"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/acaduser/"
	},
	"icon": "images/icon.png",
	"license": "",
	"keywords": [
		"autolisp",
		"lisp",
		"lsp",
		"autocad"
	],
	"contributes": {
		"languages": [
			{
				"id": "autolisp",
				"aliases": [
					"Autolisp",
					"autolisp"
				],
				"extensions": [
					".lsp",
					".mnl"
				],
				"configuration": "./language-configuration.json"
			},
			{
				"id": "autolispdcl",
				"aliases": [
					"AutolispDCL",
					"autolispdcl"
				],
				"extensions": [
					".dcl"
				],
				"configuration": "./language-configuration-dcl.json"
			}
		],
		"grammars": [
			{
				"language": "autolisp",
				"scopeName": "source.autolisp",
				"path": "./syntaxes/autolisp.tmLanguage.json"
			},
			{
				"language": "autolispdcl",
				"scopeName": "source.autolispdcl",
				"path": "./syntaxes/autolispdcl.tmLanguage.json"
			}
		],
		"configurationDefaults": {
			"[autolisp]": {
				"editor.acceptSuggestionOnEnter": "off",
				"editor.wordWrap": "on",
				"editor.wordBasedSuggestions": false,
				"editor.quickSuggestions": true,
				"editor.insertSpaces": false,
				"editor.tabSize": 4,
				"files.encoding": "utf8"
			},
			"[autolispdcl]": {
				"editor.acceptSuggestionOnEnter": "off",
				"editor.wordWrap": "on",
				"editor.wordBasedSuggestions": false,
				"editor.quickSuggestions": true,
				"editor.insertSpaces": false,
				"editor.tabSize": 4,
				"files.encoding": "utf8"
			}
		},
		"themes": [
			{
				"label": "autolisp light theme",
				"uiTheme": "vs",
				"path": "./themes/autolisp light-color-theme.json"
			}
		],
		"snippets": [
			{
				"language": "autolispdcl",
				"path": "./snippets/snippets-dcl.json"
			},
			{
				"language": "autolisp",
				"path": "./snippets/snippets-lsp-const.json"
			},
			{
				"language": "autolisp",
				"path": "./snippets/snippets-lsp-support.json"
			},
			{
				"language": "autolisp",
				"path": "./snippets/snippets-lsp-symbol.json"
			},
			{
				"language": "autolisp",
				"path": "./snippets/snippets-lsp-sysvar.json"
			}
		]
	}
}
